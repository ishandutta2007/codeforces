
int main(){
  string in;
  int n;
  int k;
  int n1;
  in=Stdio.stdin->gets();
  sscanf(in,"%d%d%d",n,k,n1);
  if (n==n1) {
    if (k>=1){
      write("YES\n");
    } else {
      write("NO\n");
    }
    return 0;
  }
  if (k>=4) {
    write("YES\n");
  } else {
    write("NO\n");
  }
  return 0;
}