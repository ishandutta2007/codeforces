n := File standardInput readLine asNumber
s := 2;
u := 0;
i := 0;
des := 1;
while (1 < 2,
  chek := 0;
  h := n;
  des := 1;
  i := i + 1;
  if (i > 1,
    c := i - 1
    c repeat(des := des * 10)
  )
  h := (h - h % des) / des
  if (h > 0, if (h % 10 == 0, chek := 1))
  h := h % 10
  //h println
  if (chek == 0,
  if (h == 0, 
    if (u == 0, "upset" println,
      if (s == 1, "happy" println, "happier" println) 
    )
  break))
  h := h % 10;
  if (n % h == 0, u := 1, s := 1)
)