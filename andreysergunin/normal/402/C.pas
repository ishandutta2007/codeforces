var a:array[1..100,1..100] of byte;
    i,j,w,t,n,p,k,s:longint;

begin
 read (t);
 for w:=1 to t do
        begin
         read (n,p);
         for i:=1 to n do
                for j:=1 to n do
                      a[i,j]:=0;

         k:=n div 5 - 1;
         for i:=1 to k do
                begin
                 a[5*i-4,5*i-3]:=1;
                 a[5*i-4,5*i-2]:=1;
                 a[5*i-4,5*i-1]:=1;
                 a[5*i-4,5*i]:=1;
                 a[5*i-3,5*i-2]:=1;
                 a[5*i-3,5*i-1]:=1;
                 a[5*i-3,5*i]:=1;
                 a[5*i-2,5*i-1]:=1;
                 a[5*i-2,5*i]:=1;
                 a[5*i-1,5*i]:=1;
                end;

                 i:=k+1;
                 if n mod 5=0 then
                        begin
                         a[5*i-4,5*i-3]:=1;
                         a[5*i-4,5*i-2]:=1;
                         a[5*i-4,5*i-1]:=1;
                         a[5*i-4,5*i]:=1;
                         a[5*i-3,5*i-2]:=1;
                         a[5*i-3,5*i-1]:=1;
                         a[5*i-3,5*i]:=1;
                         a[5*i-2,5*i-1]:=1;
                         a[5*i-2,5*i]:=1;
                         a[5*i-1,5*i]:=1;
                        end;
                 if n mod 5=1 then
                        begin
                         a[5*i-4,5*i-3]:=1;
                         a[5*i-4,5*i-2]:=1;
                         a[5*i-4,5*i-1]:=1;
                         a[5*i-4,5*i]:=1;
                         a[5*i-3,5*i-2]:=1;
                         a[5*i-3,5*i-1]:=1;
                         a[5*i-3,5*i]:=1;
                         a[5*i-2,5*i-1]:=1;
                         a[5*i-2,5*i]:=1;
                         a[5*i-1,5*i]:=1;
                         a[5*i-4,5*i+1]:=1;
                         a[5*i-3,5*i+1]:=1;
                        end;
                 if n mod 5=2 then
                        begin
                         a[5*i-4,5*i-3]:=1;
                         a[5*i-4,5*i-2]:=1;
                         a[5*i-4,5*i-1]:=1;
                         a[5*i-4,5*i]:=1;
                         a[5*i-3,5*i-2]:=1;
                         a[5*i-3,5*i-1]:=1;
                         a[5*i-3,5*i]:=1;
                         a[5*i-2,5*i-1]:=1;
                         a[5*i-2,5*i]:=1;
                         a[5*i-1,5*i]:=1;
                         a[5*i-4,5*i+1]:=1;
                         a[5*i-3,5*i+1]:=1;
                         a[5*i-3,5*i+2]:=1;
                         a[5*i-2,5*i+2]:=1;
                        end;
                 if n mod 5=3 then
                        begin
                         a[5*i-4,5*i-3]:=1;
                         a[5*i-4,5*i-2]:=1;
                         a[5*i-4,5*i-1]:=1;
                         a[5*i-4,5*i]:=1;
                         a[5*i-3,5*i-2]:=1;
                         a[5*i-3,5*i-1]:=1;
                         a[5*i-3,5*i]:=1;
                         a[5*i-2,5*i-1]:=1;
                         a[5*i-2,5*i]:=1;
                         a[5*i-1,5*i]:=1;
                         a[5*i-4,5*i+1]:=1;
                         a[5*i-3,5*i+1]:=1;
                         a[5*i-3,5*i+2]:=1;
                         a[5*i-2,5*i+2]:=1;
                         a[5*i-2,5*i+3]:=1;
                         a[5*i-1,5*i+3]:=1;
                        end;
                 if n mod 5=4 then
                        begin
                         a[5*i-4,5*i-3]:=1;
                         a[5*i-4,5*i-2]:=1;
                         a[5*i-4,5*i-1]:=1;
                         a[5*i-4,5*i]:=1;
                         a[5*i-3,5*i-2]:=1;
                         a[5*i-3,5*i-1]:=1;
                         a[5*i-3,5*i]:=1;
                         a[5*i-2,5*i-1]:=1;
                         a[5*i-2,5*i]:=1;
                         a[5*i-1,5*i]:=1;
                         a[5*i-4,5*i+1]:=1;
                         a[5*i-3,5*i+1]:=1;
                         a[5*i-3,5*i+2]:=1;
                         a[5*i-2,5*i+2]:=1;
                         a[5*i-2,5*i+3]:=1;
                         a[5*i-1,5*i+3]:=1;
                         a[5*i-1,5*i+4]:=1;
                         a[5*i,5*i+4]:=1;
                        end;

         s:=0;
         for i:=1 to n do
                begin
                 for j:=1 to n do
                        begin
                         if (a[i,j]=0) and (a[j,i]=0) and (s<p) and (i<>j) then
                                begin
                                 inc (s);
                                 a[i,j]:=1;
                                end;
                        end;
                end;
         for i:=1 to n do
                for j:=1 to n do if a[i,j]=1 then writeln (i,' ',j);
        end;
end.