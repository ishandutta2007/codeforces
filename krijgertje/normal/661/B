s=:(1!:1)3
s=:s-.(LF,CR)
months=:'January','February','March','April','May','June','July','August','September','October','November',:'December'
seasons=:'winter','winter','spring','spring','spring','summer','summer','summer','autumn','autumn','autumn',:'winter'
