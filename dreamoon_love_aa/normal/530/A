main =>
    A=read_int(),
    B=read_int(),
    C=read_int(),
    D=B*B-4*A*C,
    if (D>0) then
        if (A>0) then
            printf("%.4f %.4f\n",((-1)*B-sqrt(D))/(2*A),((-1)*B+sqrt(D))/(2*A))
        else
            printf("%.4f %.4f\n",((-1)*B+sqrt(D))/(2*A),((-1)*B-sqrt(D))/(2*A))
        end
    else
        writef("%.4f\n",((-1)*B)/(2*A))
    end.