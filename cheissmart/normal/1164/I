#include <bits/stdc++.h>
#define IO_OP std::ios::sync_with_stdio(0); std::cin.tie(0);
#define F first
#define S second
#define PB push_back
#define MP make_pair
#define EB emplace_back

using namespace std;

typedef long long ll;
typedef pair<int, int> pi;
typedef vector<int> vi;

const int INF = 1e9 + 7;

int main()
{
	IO_OP;
	
	int s, v1, v2, t1, t2, d1, d2;
	cin >> s >> v1 >> v2 >> t1 >> t2;
	d1 = s * v1 + t1 * 2, d2 = s * v2 + t2 * 2;
	if(d1 < d2)
		cout << "First" << endl;
	else if(d1 > d2) 
		cout << "Second" << endl;
	else
		cout << "Friendship" << endl;
}