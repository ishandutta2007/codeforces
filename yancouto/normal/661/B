print =: 1!:2&2
read =: 1!:1[3
NB. n =. ". (read-.LF)-.CR
NB. n2=. 1+((n*(n+1))%2)
NB. echo n2
d =. (read-.LF)-.CR
f =. (#d) 
L =. 'OFhi---g-OND' e. d
S =. 7 0 0 0 3 4 4 0 9 0 0 0 e. (#d)
Q =. '-----le-----' e. d


LS =. L +. S
LS =. LS *. (-. (L *. S))

LS =. LS *. (-. Q)
se =. 'winter'; 'spring'; 'summer'; 'autumn'
mo =. 0 0 1 1 1 2 2 2 3 3 3 0
echo ;((LS # mo){se)

exit ''